

object nameGenerator {
  def main(args: Array[String]): Unit = {
    val maj = Set('A' to 'Z')
    val min = Set('a' to 'z')
    val vowel_maj = Set('A', 'E', 'I', 'O', 'U', 'Y')
    val vowel_min = Set('a', 'e', 'i', 'o', 'u', 'y')
    val conso_maj = maj.filterNot(vowel_maj)
    println(conso_maj)    
  }
  
}