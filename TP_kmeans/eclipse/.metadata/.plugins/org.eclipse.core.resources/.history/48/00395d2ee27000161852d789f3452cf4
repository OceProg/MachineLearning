

object nameGenerator {
  def main(args: Array[String]): Unit = {
    val maj = Set('A' to 'Z')
    val min = List('a' to 'z')
    val vowel_maj = List('A', 'E', 'I', 'O', 'U', 'Y')
    val vowel_min = List('a', 'e', 'i', 'o', 'u', 'y')
    val conso_maj = maj.filterNot(vowel_maj.contains(_))
    val p = vowel_maj.toSet
    println(conso_maj)    
  }
  
}